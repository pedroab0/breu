{
  "$schema": "vscode://schemas/color-theme",
  "name": "Breu",
  "semanticClass": "theme.breu",
  "semanticHighlighting": true,
  "breu": {
    "base": [
      "#000000",
      "#F8F8F2",
      "#44475A",
      "#6272A4",
      "#8BE9FD",
      "#50FA7B",
      "#FFB86C",
      "#FF79C6",
      "#BD93F9",
      "#FF5555",
      "#F1FA8C",
      "#179FFF"
    ],
    "ansi": [
      "#21222C",
      "#FF5555",
      "#50FA7B",
      "#F1FA8C",
      "#BD93F9",
      "#FF79C6",
      "#8BE9FD",
      "#F8F8F2",
      "#6272A4",
      "#FF6E6E",
      "#69FF94",
      "#FFFFA5",
      "#D6ACFF",
      "#FF92DF",
      "#A4FFFF",
      "#FFFFFF"
    ],
    "ui": [
      "#181A1F",
      "#21252B",
      "#2c313a",
      "#9da5b4",
      "#6B717D",
      "#495162",
      "#CCA700"
    ]
  },
  "semanticTokenColors": {
    "class": "#179FFF",
    "interface": "#FF79C6",
    "type": "#FF79C6",
    "function": "#50FA7B",
    "method": "#50FA7B",
    "parameter": {
      "foreground": "#FFB86C",
      "fontStyle": "italic"
    },
    "variable.readonly": "#BD93F9",
    "property.readonly": "#BD93F9"
  },
  "colors": {
